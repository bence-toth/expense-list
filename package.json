{
  "name": "expense-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "lint": "npm run eslint ; npm run stylelint",
    "eslint": "node ./node_modules/eslint/bin/eslint.js 'src/**/*.js' 'src/**/*.jsx'",
    "stylelint": "./node_modules/stylelint/bin/stylelint.js \"src/**/*.css\"",
    "cypress": "cypress open"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.utility.js",
      "src/**/*.actionCreators.js",
      "src/**/*.reducer.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "md5": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-input-range": "^1.3.0",
    "react-quill": "^1.3.3",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@agillic/eslint-config": "0.0.5",
    "cypress": "^3.7.0",
    "eslint": "^6.5.1",
    "mocha-each": "^2.0.1",
    "stylelint": "^11.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
